{
  "version": 2,
  "builds": [
    {
      "src": "server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*", 
      "dest": "server.js"
    },
    {
      "src": "/varnauthsava/(.*)", 
      "dest": "server.js"
    },
    {
      "src": "/image/(.*)", 
      "dest": "server.js"
    }
  ]
}